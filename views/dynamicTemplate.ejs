
<!--this is a dynamic template for generation of html pages with similar designs antique silver utensil page is
generated from it -->

<div class="container my-2"><!--antique dinner set-->
    <div class="row">
        <% var i = 0; %>
        <% imageObjectArrayInEjs.forEach(obj=>{ %>
            
            <div class="col-xl-6 col-lg-6 col-md-12 col-sm-12 mx-auto my-2"><!--2nd column-->
                <h6 class="text-center text-secondary text-capitalize">
                    <%=obj.imageTitle;%>
                </h6>
                <hr>   
                <div class="container">
                    <div class="row">
                        <div class="col-7 border-end border-secondary rounded"> 
                            <picture>
                                <source media="(max-width:799px)" srcset="<%=obj.srcSetMobile; %>" >
                                <source media="(min-width:800px)" srcset=<%=obj.srcSetWeb;%> >
                                <img class="img-fluid" alt="Antique Dinner Set" data-url="silverUtensils" data-category="<%=obj.imageTitle;%>" src="<%=obj.srcSetMobile; %>">
                            </picture>   
                            
                        </div>
                        <div class="col-5">
                            
                            <p class="text-secondary text-capitalize" style="font-size: small;">
                               <span class=" lh-lg"> Description : </span><br>
                                <%= obj.imageDesc.desc ; %>
                            </p>
                            <p class="text-secondary text-capitalize shadow p-3 mb-5 bg-white rounded wideViewIndex" style="font-size: small;">
                                <a  href="#fetchDiv<%=i;%>" role="button" class="btn text-secondary text-capitalize singleClickBtn" style="font-size: small;" id="<%=obj.imageId+i;%>" data-fetch-string="<%=obj._fetchTag%>" type="submit" onclick="getImage(event)">
                                    explore gallery of <%= obj.imageTitle; %>
                                </a>
                            </p>
                        </div>
                    </div>
                    <div class="container">
                       
                        <p class="text-capitalize text-secondary" style="font-size: small;">
                            <span class=" lh-lg">Size :</span> <%= obj.imageDesc.dimension; %> <br>
                            <span class=" lh-lg">components :</span> <%= obj.imageDesc.components; %>
                            <a class="mx-2 shadow p-1 mb-5 bg-white rounded" data-bs-toggle="collapse"  role="button" aria-expanded="false" aria-controls="<%="collapseExample"+i; %>" href="<%="#collapseExample"+i; %>">
                                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" fill="#cccc00" class="bi bi-info-circle" viewBox="0 0 16 16">
                                    <path d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z"/>
                                    <path d="m8.93 6.588-2.29.287-.082.38.45.083c.294.07.352.176.288.469l-.738 3.468c-.194.897.105 1.319.808 1.319.545 0 1.178-.252 1.465-.598l.088-.416c-.2.176-.492.246-.686.246-.275 0-.375-.193-.304-.533L8.93 6.588zM9 4.5a1 1 0 1 1-2 0 1 1 0 0 1 2 0z"/>
                                </svg>
                            </a>
                            <div class="collapse" id="<%="collapseExample"+i; %>">
                                <div class="card card-body text-capitalize text-secondary" style="font-size: small;">
                                  <%= obj.imageDesc.additionalImageInfo; %>
                                </div>
                              </div>
                            
                        </p>
                        <p class="text-capitalize text-secondary lh-lg" style="font-size: smaller;">
                            <!--category(var)-->category :
                            <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" fill="blue" class="bi bi-link" viewBox="0 0 16 16">
                                <path d="M6.354 5.5H4a3 3 0 0 0 0 6h3a3 3 0 0 0 2.83-4H9c-.086 0-.17.01-.25.031A2 2 0 0 1 7 10.5H4a2 2 0 1 1 0-4h1.535c.218-.376.495-.714.82-1z"/>
                                <path d="M9 5.5a3 3 0 0 0-2.83 4h1.098A2 2 0 0 1 9 6.5h3a2 2 0 1 1 0 4h-1.535a4.02 4.02 0 0 1-.82 1H12a3 3 0 1 0 0-6H9z"/>
                              </svg><br>
                              <% var k=0;%>
                              <% obj.category.forEach(cat=>{%>
                                
                                <a href="<%=obj._url[k]%>" ><%= cat; %></a> <br>
                              <% k+=1; });%>
                              
                            <!--<br>antique silver utensils,<br> dinner set,<br> antique dinner set,<br> silver utensils-->
                        </p>
                    </div>
                    <div class="container" >
                        <p class="text-secondary text-capitalize shadow p-3 mb-5 bg-white rounded mobileViewIndex" style="font-size: small;">
                            
                            <button class="btn text-secondary text-capitalize" style="font-size: small;" id="<%=obj.imageId;%>" data-fetch-string="<%=obj._fetchTag%>" title="<%= obj.imageTitle;%>" type="submit" onclick="getImage(event)">
                                explore gallery of <%= obj.imageTitle; %>
                            </button>
                        </p>
                    </div>
                </div> 
                <!--container to display fetched image -->
                <div id="fetchDiv<%=i;%>" ></div>

                <div class="container my-1 d-none" id="<%=obj._fetchTag;%>">
                    <h6 class="text-capitalize text-secondary p-2 my-1">gallery of <%=obj.imageTitle;%></h6>
                    
                    <hr>
                    <div class="container scrollWebKit <%=obj._fetchTag;%>  shadow p-3 mb-5 bg-white rounded " style="overflow: auto; white-space: nowrap;">
                        
                    </div>
                    
                </div>  <!--fetch container div close--> 
                
                 
            </div><!--end of 1st column-->

            
           

        <% i+=1; }) ; %><!--this ends the top forEach function and the variable i manipulates the id's so that id's are unique-->

        
            

       
